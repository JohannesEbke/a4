prefix=$(prefix)
exec_prefix=$(exec_prefix)
includedir=$(includedir)
libdir=$(libdir)

Name: A4
Description: Another Attempt At Analysis
URL: https://github.com/JohannesEbke/a4
Version: @PACKAGE_VERSION@
Cflags: -std=c++0x -I${includedir} @PROTOBUF_CFLAGS@ @BOOST_CPPFLAGS@ @SNAPPY_CPPFLAGS@
Libs: -L${libdir} -la4io -la4process -la4root @PROTOBUF_LIBS@ @BOOST_LIBS@ @SNAPPY_LDFLAGS@
Requires: protobuf >= 2.4
