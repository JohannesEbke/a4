
install-data-local-protobuf:
	if test "x$(DESTDIR)" != "x"; then DESTDIROPT="--root=$(DESTDIR)"; fi
	if test "x$(PROTOBUF_BUILD)" != "x"; then \
	    cd $(srcdir)/$(PROTOBUF_BUILD)/python;\
	    PATH=$(PROTOBUF_PRIVATE_BUILDDIR):$(PATH) PYTHONPATH=$(PYTHONPATH):$(prefix)/lib/python2.6/site-packages $(PYTHON) setup.py install --prefix=$(prefix) $(DESTDIROPT); \
	fi

install-data-local: install-data-local-protobuf

SUBDIRS+=$(PROTOBUF_BUILD)
